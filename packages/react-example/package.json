{
  "name": "react-example",
  "description": "Example using Workux",
  "private": true,
  "scripts": {
    "build": "npm run build:clean && npm run build:production",
    "build:clean": "./node_modules/.bin/rimraf build",
    "build:production": "./node_modules/.bin/webpack --colors --display-error-details --config ./webpack.config.prod.js",
    "build:serve": "./node_modules/.bin/http-server ./build",
    "start": "npm run start:clean && npm run start:webpack",
    "start:clean": "./node_modules/.bin/rimraf build",
    "start:webpack": "node ./webpack.server.js"
  },
  "dependencies": {
    "apollo-client": "1.4.2",
    "babel-core": "6.25.0",
    "babel-loader": "7.0.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-pure-class-to-function": "1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.6",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.5.2",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.23.0",
    "graphql": "0.10.1",
    "graphql-tag": "2.4.0",
    "html-webpack-plugin": "2.28.0",
    "lodash": "4.17.4",
    "react": "15.6.1",
    "react-apollo": "1.4.2",
    "react-dom": "15.6.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "5.0.5",
    "redux": "3.7.0",
    "redux-actions": "2.0.3",
    "redux-saga": "0.15.3",
    "rimraf": "2.6.1",
    "webpack": "2.6.1",
    "worker-loader": "0.8.0",
    "workux": "file:../workux"
  },
  "devDependencies": {
    "http-server": "0.10.0",
    "webpack-dev-server": "2.4.5"
  }
}